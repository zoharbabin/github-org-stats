{
  "authentication": {
    "app_id": 12345,
    "private_key_path": "/path/to/private-key.pem",
    "installation_mappings": {
      "your-org": 67890,
      "another-org": 11111,
      "third-org": 22222
    }
  },
  "analysis": {
    "days_back": 60,
    "max_repos": 200,
    "include_forks": false,
    "include_archived": false,
    "exclude_bots": true,
    "include_empty": false
  },
  "output": {
    "format": "excel",
    "output_dir": "./reports",
    "timezone": "UTC"
  },
  "logging": {
    "level": "INFO",
    "file": "github_analysis.log"
  },
  "filtering": {
    "repositories": [
      "specific-repo-1",
      "specific-repo-2"
    ],
    "exclude_patterns": [
      "test-*",
      "*-backup",
      "*-archive"
    ]
  },
  "advanced_features": {
    "dependency_analysis": true,
    "submodule_detection": true,
    "github_actions_analysis": true,
    "branch_protection_check": true,
    "contributor_analysis": true,
    "code_quality_metrics": true,
    "security_insights": true,
    "release_tracking": true
  },
  "rate_limiting": {
    "buffer": 100,
    "retry_attempts": 3,
    "backoff_factor": 2.0,
    "delay": 1.0
  },
  "excel_output": {
    "batch_size": 100,
    "max_cell_length": 32767,
    "include_formatting": true,
    "multiple_sheets": true
  }
}